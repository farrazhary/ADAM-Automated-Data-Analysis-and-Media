# 🌐 n8n basic config
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_TIMEZONE=Asia/Jakarta
GENERIC_TIMEZONE=Asia/Jakarta
WEBHOOK_URL=https://your-ngrok-domain

# 🤖 Google Gemini AI
GEMINI_API_KEY=your-gemini-api-key-here

# 🎥 Vertex AI (incl. VEO 3 for video generation)
VERTEX_PROJECT_ID=your-gcp-project-id
VERTEX_LOCATION=us-central1
VERTEX_API_KEY=your-vertex-ai-api-key-here

# 🗄️ Supabase (Postgres)
SUPABASE_URL=https://your-supabase-project
SUPABASE_ANON_KEY=your-supabase-anon-key-here
SUPABASE_SERVICE_KEY=your-supabase-service-role-key-here

# 🎙️ ElevenLabs (TTS + Voice Changer)
ELEVENLABS_API_KEY=your-elevenlabs-api-key-here

# 📲 WhatsApp Business API
WHATSAPP_PHONE_NUMBER_ID=your-whatsapp-number-id
WHATSAPP_BUSINESS_ACCOUNT_ID=your-business-account-id
WHATSAPP_ACCESS_TOKEN=your-whatsapp-access-token

# 🌍 Ngrok
NGROK_AUTHTOKEN=your-ngrok-authtoken-here
NGROK_DOMAIN=your-static-ngrok-domain-here
